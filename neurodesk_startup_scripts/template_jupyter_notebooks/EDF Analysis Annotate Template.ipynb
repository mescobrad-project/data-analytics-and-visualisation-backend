    "import mne\n",
    "import time\n",
    "\n",
    "%matplotlib qt5\n",
    "\n",
    "data = mne.io.read_raw_edf('', infer_types=True, preload = True)\n",
    "data = data.notch_filter(freqs = 70)\n",
    "fig = data.plot(n_channels=50)\n",
    "\n",
    "while 1:\n",
    "    data.annotations.save(fname=\"annotation_test.csv\", overwrite=True)\n",
    "    time.sleep(5)\n"
